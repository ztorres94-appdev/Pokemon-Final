<h1> Choose a set </h1>


<table class="table" border="1">
  <tr>
    <th>
    </th>
    <th>
      Set Name
    </th>
        <% #Blank %>
    <th>
      Series
    </th>

    <th>
     Release Date
    </th>
    <th>
     Set List
    </th>

  </tr>

  <% @list_of_sets.each do |a_set| %>
  <tr>
    <th>
   <img src= <%= a_set.images.logo %> width="200" height="100" alt="" >
    </th>
    <th>
    <%= a_set.name %>
    </th>
     <th>
    <%= a_set.series %>
    </th>
     <th>
    <%= a_set.release_date %>
    </th>

    <th>
    <a href="/sets/<%= a_set.id %>" >
    View this set list 
    </th>


  </tr>
  <% end %>
</table>